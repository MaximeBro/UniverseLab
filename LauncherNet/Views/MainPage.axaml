<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:LauncherNet.ViewModels"
             x:Class="LauncherNet.Views.MainPage"
             x:DataType="vm:MainPageViewModel">
    
    <Grid RowDefinitions="Auto, Auto, *">
        <Border Grid.Row="0" Background="#00000026" Height="25">
            
        </Border>
        
        <Border Grid.Row="1" Margin="30 20" Background="MediumPurple" BorderBrush="White" CornerRadius="4" Height="100">
            <Button Command="{Binding LoginCommand}">Login</Button>
        </Border>
    </Grid>
    
</UserControl>
